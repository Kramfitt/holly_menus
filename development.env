# Copy this to .env for local development
FLASK_ENV=development
FLASK_DEBUG=1

# Supabase - Use test project or development credentials
SUPABASE_URL=your_dev_supabase_url
SUPABASE_KEY=your_dev_supabase_key

# Redis (mock will be used if not available)
REDIS_URL=redis://localhost:6379

# SMTP - Use test email service or development credentials
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_test_email@gmail.com
SMTP_PASSWORD=your_app_specific_password

# App Settings
SECRET_KEY=dev-secret-key
DASHBOARD_PASSWORD=admin

# Development Settings
DEBUG=True
TESTING=True 